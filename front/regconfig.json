{
  "core": {
    "workingDir": ".reg",
    "actualDir": "actual_img",
    "thresholdRate": 0,
    "addIgnore": true,
    "ximgdiff": {
      "invocationType": "client"
    }
  },
  "plugins": {
    "reg-keygen-git-hash-plugin": true,
    "reg-notify-github-plugin": {
      "clientId": "$REG_NOTIFY_GITHUB_PLUGIN_CLIENT_ID"
    },
    "reg-publish-s3-plugin": {
      "bucketName": "$REG_PUBLISH_S3_PLUGIN_BUCKET_NAME"
    }
  }
}
